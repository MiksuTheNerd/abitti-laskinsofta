{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["ES2023"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "jsx": "react-jsx"
  },
  "include": ["src/**/*"]
}
